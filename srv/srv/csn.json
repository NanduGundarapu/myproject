    "FLIGHT.SflightView": {
      "kind": "entity",
      "query": {
        "SELECT": {
          "from": {
            "ref": [
              "SFLIGHT"
            ],
            "as": "FLIGHTTBL"
          },
          "mixin": {
            "SBOOKLink": {
              "type": "cds.Association",
              "cardinality": {
                "max": "*"
              },
              "target": "SBOOK",
              "on": [
                {
                  "ref": [
                    "SBOOKLink",
                    "MANDT"
                  ]
                },
                "=",
                {
                  "ref": [
                    "FLIGHTTBL",
                    "MANDT"
                  ]
                },
                "and",
                {
                  "ref": [
                    "SBOOKLink",
                    "CARRID"
                  ]
                },
                "=",
                {
                  "ref": [
                    "FLIGHTTBL",
                    "CARRID"
                  ]
                },
                "and",
                {
                  "ref": [
                    "SBOOKLink",
                    "CONNID"
                  ]
                },
                "=",
                {
                  "ref": [
                    "FLIGHTTBL",
                    "CONNID"
                  ]
                },
                "and",
                {
                  "ref": [
                    "SBOOKLink",
                    "FLDATE"
                  ]
                },
                "=",
                {
                  "ref": [
                    "FLIGHTTBL",
                    "FLDATE"
                  ]
                }
              ]
            }
          },
          "columns": [
            {
              "ref": [
                "MANDT"
              ],
              "as": "Client"
            },
            {
              "ref": [
                "CARRID"
              ],
              "as": "CarrierId"
            },
            {
              "ref": [
                "CONNID"
              ],
              "as": "ConnectionId"
            },
            {
              "ref": [
                "FLDATE"
              ],
              "as": "FlightDate"
            },
            {
              "ref": [
                "PRICE"
              ],
              "as": "Price"
            },
            {
              "ref": [
                "CURRENCY"
              ],
              "as": "Currency"
            },
            {
              "ref": [
                "PLANETYPE"
              ],
              "as": "PlaneType"
            },
            {
              "ref": [
                "SBOOKLink",
                "BOOKID"
              ],
              "as": "BookingId"
            },
            {
              "ref": [
                "SBOOKLink",
                "CUSTOMID"
              ],
              "as": "CustomerId"
            },
            {
              "ref": [
                "SBOOKLink",
                "CUSTTYPE"
              ],
              "as": "CustomerType"
            },
            {
              "ref": [
                "SBOOKLink",
                "PASSNAME"
              ],
              "as": "PassengerName"
            }
          ]
        }
      },
      "elements": {
        "Client": {
          "@title": "MANDT",
          "type": "cds.String",
          "length": 3
        },
        "CarrierId": {
          "@title": "CARRID",
          "type": "cds.String",
          "length": 3
        },
        "ConnectionId": {
          "@title": "CONNID",
          "type": "cds.String",
          "length": 4
        },
        "FlightDate": {
          "@title": "FLDATE",
          "type": "cds.String",
          "length": 8
        },
        "Price": {
          "@title": "PRICE",
          "type": "cds.Decimal",
          "precision": 15,
          "scale": 2,
          "notNull": true
        },
        "Currency": {
          "@title": "CURRENCY",
          "type": "cds.String",
          "length": 5,
          "notNull": true
        },
        "PlaneType": {
